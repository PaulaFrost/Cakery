@inherits UmbracoViewPage<ContentModels.Category>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = "master.cshtml";
}
@Html.Partial("~/Views/Partials/SectionHeader.cshtml")
<section class="section">

    <div class="container">
        <div class="product-grid">
            @if (Model.ProductCategoriesList != null)
            {
                foreach (ProductCategory productC in Model.ProductCategoriesList)
                {
                  <a href="@productC.Url" class="product-grid__item" style="background-image: url('@productC.ProductCategoryImage.Url')">
                        <div class="product-grid__item__overlay">
                            <div class="product-grid__item__name">@productC.ProductCategoryName</div>
                           
                        </div>
                    </a>
                }
            }
        </div>
    </div>

</section>

